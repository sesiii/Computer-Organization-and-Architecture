S_0x5876e84cbbd0 .scope module, "tb_divison" "tb_divison" 2 4;
 .timescale 0 0;
v0x5876e84e03c0_0 .var "d", 7 0;
v0x5876e84e04a0_0 .var "n", 7 0;
v0x5876e84e0570_0 .net "q", 7 0, v0x5876e84dfe80_0;  1 drivers
v0x5876e84e0670_0 .net "r", 7 0, v0x5876e84dff70_0;  1 drivers
S_0x5876e84cbd60 .scope module, "uut" "divison" 2 10, 3 4 0, S_0x5876e84cbbd0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "n";
    .port_info 1 /INPUT 8 "d";
    .port_info 2 /OUTPUT 8 "q";
    .port_info 3 /OUTPUT 8 "r";
v0x5876e84b9040_0 .net "d", 7 0, v0x5876e84e03c0_0;  1 drivers
v0x5876e84dfda0_0 .net "n", 7 0, v0x5876e84e04a0_0;  1 drivers
v0x5876e84dfe80_0 .var "q", 7 0;
v0x5876e84dff70_0 .var "r", 7 0;
v0x5876e84e0050_0 .var "temp_d", 7 0;
v0x5876e84e0180_0 .var "temp_q", 7 0;
v0x5876e84e0260_0 .var "temp_r", 7 0;
E_0x5876e84c9cf0/0 .event anyedge, v0x5876e84dfda0_0, v0x5876e84b9040_0, v0x5876e84e0260_0, v0x5876e84e0050_0;
E_0x5876e84c9cf0/1 .event anyedge, v0x5876e84e0180_0;
E_0x5876e84c9cf0 .event/or E_0x5876e84c9cf0/0, E_0x5876e84c9cf0/1;
    .scope S_0x5876e84cbd60;
T_0 ;
    %wait E_0x5876e84c9cf0;
    %load/vec4 v0x5876e84dfda0_0;
    %store/vec4 v0x5876e84e0260_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5876e84e0180_0, 0, 8;
    %load/vec4 v0x5876e84b9040_0;
    %inv;
    %addi 1, 0, 8;
    %store/vec4 v0x5876e84e0050_0, 0, 8;
T_0.0 ;
    %load/vec4 v0x5876e84b9040_0;
    %load/vec4 v0x5876e84e0260_0;
    %cmp/u;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5876e84e0260_0;
    %load/vec4 v0x5876e84e0050_0;
    %add;
    %store/vec4 v0x5876e84e0260_0, 0, 8;
    %load/vec4 v0x5876e84e0180_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5876e84e0180_0, 0, 8;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x5876e84e0260_0;
    %store/vec4 v0x5876e84dff70_0, 0, 8;
    %load/vec4 v0x5876e84e0180_0;
    %store/vec4 v0x5876e84dfe80_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5876e84cbbd0;
T_1 ;
    %delay 10, 0;
    %pushi/vec4 100, 0, 8;
    %store/vec4 v0x5876e84e04a0_0, 0, 8;
    %pushi/vec4 30, 0, 8;
    %store/vec4 v0x5876e84e03c0_0, 0, 8;
    %vpi_call 2 16 "$monitor", "Time: %0t | N= %d | D= %d | ==>> | Q=%d | R=%d |", $time, v0x5876e84e04a0_0, v0x5876e84e03c0_0, v0x5876e84e0570_0, v0x5876e84e0670_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 12, 0, 8;
    %store/vec4 v0x5876e84e04a0_0, 0, 8;
    %pushi/vec4 13, 0, 8;
    %store/vec4 v0x5876e84e03c0_0, 0, 8;
    %vpi_call 2 21 "$monitor", "Time: %0t | N= %d | D= %d | ==>> | Q=%d | R=%d |", $time, v0x5876e84e04a0_0, v0x5876e84e03c0_0, v0x5876e84e0570_0, v0x5876e84e0670_0 {0 0 0};
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_1;
