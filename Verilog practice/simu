#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5e4a2a559540 .scope module, "for_loop_example" "for_loop_example" 2 1;
 .timescale 0 0;
v0x5e4a2a55b400 .array "data", 7 0, 3 0;
v0x5e4a2a5596f0_0 .var/i "i", 31 0;
    .scope S_0x5e4a2a559540;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5e4a2a5596f0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5e4a2a5596f0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5e4a2a5596f0_0;
    %pad/s 4;
    %ix/getv/s 4, v0x5e4a2a5596f0_0;
    %store/vec4a v0x5e4a2a55b400, 4, 0;
    %load/vec4 v0x5e4a2a5596f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5e4a2a5596f0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5e4a2a5596f0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5e4a2a5596f0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.3, 5;
    %vpi_call 2 13 "$display", "data[%0d] = %100b", v0x5e4a2a5596f0_0, &A<v0x5e4a2a55b400, v0x5e4a2a5596f0_0 > {0 0 0};
    %load/vec4 v0x5e4a2a5596f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5e4a2a5596f0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
